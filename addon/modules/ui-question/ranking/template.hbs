{{#each options as |option index|}}
    <div class="ranking-rank {{option.sortStateClass}}" onclick={{action "test" index}}>
      <div class="ranking-number">{{option.sortNo}}</div>
      {{ui-addon/label
        uuid=option.uuid
        text=option.text
        svgIcon=option.icon
      }}
    </div>
{{/each}}