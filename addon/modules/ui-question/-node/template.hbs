{{ui-component/ui-header headerData}}
<div class={{concat "control " quesType}}>
  {{#if isSpecialComponent}}
    {{component componentName node handleEvents options=(unbound node.options) matrix=(unbound node.matrix)}}
  {{else}}
    {{#each node.options as |option|}}
      {{component componentName node option handleEvents}}
    {{/each}}
  {{/if}}
</div>

